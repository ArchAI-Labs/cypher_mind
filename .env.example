NEO4J_URI = bolt://localhost:7687
NEO4J_USER = neo4j
NEO4J_PASSWORD =  <your-neo4j-password>

QDRANT_COLLECTION = neo4j_query_cache # you may want to rename this.
QDRANT_MODE = docker
QDRANT_URL = http://localhost:6333

# Qdrant Optimization Settings
ENABLE_QUANTIZATION = true
QUANTIZATION_TYPE = scalar  # Options: scalar, binary (binary requires vector_size >= 512)
QDRANT_ON_DISK = false  # Set to true for large datasets to save RAM
HNSW_M = 16  # Higher = better recall, more memory (default: 16, range: 4-64)
HNSW_EF_CONSTRUCT = 100  # Higher = better index quality, slower indexing (default: 100)

# Vector Embedding Settings
VECTOR_SIZE = 384
EMBEDDER = BAAI/bge-small-en-v1.5
MODEL_FILE = onnx/model.onnx
THRESHOLD = 0.999

# Batch Operations & Performance
BATCH_SIZE = 100  # Number of queries to insert in a single batch
MAX_CACHE_SIZE = 10000  # Maximum embedding cache entries
MAX_RECENT_RESULTS = 10  # Number of recent results to keep in memory (faster access)

GEMINI_API_KEY = <your-api-key>
MODEL = gemini-2.0-flash

NODE_URL = data_fake/nodes_schema.json # change with your csv path or delete if you already have a graph
RELATIONSHIP_URL = data_fake/relations_schema.json # change with your csv path or delete if you already have a graph
SAMPLE_QUESTIONS = data_fake/sample_questions.json # load sample question in streamlit
RESET = true

NODE_CONTEXT_URL = data_fake/node_context.json
REL_CONTEXT_URL = data_fake/relationship_context.json